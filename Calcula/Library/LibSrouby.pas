procedure TSrouby.EdSroNormCreate(Sender: TObject);
var pocitam:integer;
begin
  for pocitam:=0 to SroNormP-1 do EdSroNorm.Items.Add(SroNorm[pocitam]);
end;

procedure TSrouby.EdSroZavCreate(Sender: TObject);
var pocitam:integer;
begin
  for pocitam:=0 to SroZaviP-1 do EdSroZav.Items.Add(SroZavi[Pocitam].Zavit);
end;

procedure TSrouby.EdSroZavColorItem(Control: TWinControl; Index: Integer;
  var Color: TColor);
var jj:word;
begin
  try
    if EdSroNorm.ItemIndex >= 0 then begin
      { zobrazeni pripustnych zavitu }
      jj:=0;
      while jj < SroHlavP do begin
        if SroZavi[Index].d = SroHlav[jj].zavit then begin
          if SroHlav[jj].hmohla[EdSroNorm.ItemIndex] > 0
            then Color:=clBlack else Color:=clSilver;
          break;
        end;
        jj:=jj+1;
      end;
    end;
  except
  end;
end;

procedure TSrouby.EdSroDelkaCreate(Sender: TObject);
var pocitam:integer;
begin
  for pocitam:=0 to SroDelkP-1 do EdSroDelka.Items.Add(inttostr(SroDelk[0][Pocitam].l));
end;

procedure TSrouby.EdSroDelkaColorItem(Control: TWinControl; Index: Integer;
  var Color: TColor);
var jj:word;
begin
  try
    if (EdSroNorm.ItemIndex >= 0) and (EdSroZav.ItemIndex >= 0) then begin
      {zobrazeni pripustnych delek}
      jj:=0;
      while jj < SroHlavP do begin
        if SroZavi[EdSroZav.ItemIndex].d = SroHlav[jj].zavit then begin
          if SroDelk[EdSroNorm.ItemIndex][Index].b[jj] > 0
            then Color:=clBlack else Color:=clSilver;
          break;
        end;
        jj:=jj+1;
      end;
    end;
  except
  end;
end;

procedure TSrouby.SroubChange(Sender: TObject);
begin
  EdSroZav.Invalidate;
  EdSroDelka.Invalidate;
  EdSroHmot.Recompute;
end;

procedure TSrouby.EdSroHmotCompute(Sender: TObject; var Value: Extended);
var Kusu:double;
    Vysledek:Extended; { hmotnost v gramech }
    ZavPos:byte;
begin
  Kusu:=Ceil(EdSroKus.Value);  {zokrouhlí nahoru}
  if Kusu<0 then raise EMathError.Create('Poèet šroubù je záporný');

  ZavPos:=0;
  while ZavPos < SroHlavP do begin
    if SroZavi[EdSroZav.ItemIndex].d = SroHlav[ZavPos].zavit then Break;
    inc(ZavPos); { + 1 }
  end;
  if SroDelk[EdSroNorm.ItemIndex][EdSroDelka.ItemIndex].b[zavpos] = 0 then raise EMathError.Create('Požadovaný šroub není v normì');

  Vysledek:=SroHlav[zavpos].hmohla[EdSroNorm.ItemIndex]; { hmotnost hlavy}
  Vysledek:=Vysledek + SroZavi[EdSroZav.ItemIndex].hmozav * SroDelk[EdSroNorm.ItemIndex][EdSroDelka.ItemIndex].b[zavpos];
  Vysledek:=Vysledek + SroZavi[EdSroZav.ItemIndex].hmodri[EdSroNorm.ItemIndex] * ( SroDelk[EdSroNorm.ItemIndex][EdSroDelka.ItemIndex].l - SroDelk[EdSroNorm.ItemIndex][EdSroDelka.ItemIndex].b[zavpos] );

  Value:=Vysledek*Kusu/1000; { hmotnost v kilogramech }
end;

procedure TSrouby.EdSroChange(Sender: TObject);
begin
  EdSroHmot.Recompute;
end;

